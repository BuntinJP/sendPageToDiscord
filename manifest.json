{
    "manifest_version": 3,
    "name": "send to discord",
    "version": "1.0.0",
    "permissions": ["activeTab", "tabs", "storage", "contextMenus", "webRequest"],
    "action": {
        "default_icon": "icons/icon128.png",
        "default_title": "send to discord",
        "default_popup": "popup/popup.html"
    },
    "description": "send your tab to discord",
    "icons": { "48": "icons/icon48.png", "128": "icons/icon128.png" },
    "author": "Buntin"
}
