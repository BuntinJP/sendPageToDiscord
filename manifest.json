{
    "manifest_version": 3,
    "name": "send to discord",
    "version": "1.0.0",
    "permissions": ["activeTab", "scripting", "tabs", "storage", "contextMenus", "webRequest"],
    "action": {
        "default_popup": "popup/popup.html",
        "default_icon": "icons/icon128.png"
    },
    "description": "send your tab to discord",
    "icons": { "48": "icons/icon48.png", "128": "icons/icon128.png" },
    "author": "Buntin",
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+H",
                "mac": "MacCtrl+Shift+H"
            },
            "description": "Run extension."
        }
    }
}
